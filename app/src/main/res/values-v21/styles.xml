<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="MyTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="android:statusBarColor">#16ffffff</item>
    </style>


    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowExitTransition">@transition/enter</item>
        <item name="android:windowContentTransitions">true</item>
    </style>

    <style name="TheTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="android:statusBarColor">#fff</item>
    </style>

    <style name="TheTheme2" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:statusBarColor">#000</item>
        <item name="windowActionBar">false</item>
        <item name="android:colorAccent">#fff</item>
    </style>

    <style name="MyAwesomeTheme2" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/white2</item>
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="android:windowContentTransitions">true</item>
    </style>
</resources>