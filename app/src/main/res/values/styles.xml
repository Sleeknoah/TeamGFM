<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">#9d9d9d</item>
    </style>

    <style name="OurTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#7c7c7c</item>
        <item name="colorPrimaryDark">#5c5c5c</item>
        <item name="colorAccent">#f1e153</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="TheTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="windowActionBar">false</item>
    </style>

    <style name="TheTheme2" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="android:calendarViewStyle">@style/Widget.CalendarView.Custom</item>
    </style>
    <style name="Widget.CalendarView.Custom" parent="android:Widget.CalendarView">
        <item name="android:focusedMonthDateColor">#ff0008</item>
        <!--<item name="android:weekNumberColor">@color/red</item>-->
    </style>
    <style name="MyTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="windowActionBar">false</item>

    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
<style name="MyAwesomeTheme" parent="Theme.AppCompat.Light.NoActionBar">
    <item name="colorPrimary">@color/white</item>
    <item name="colorPrimaryDark">@color/teal</item>
    <item name="colorAccent">@color/yellow</item>
</style>

    <style name="MyAwesomeTheme2" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/white2</item>
        <item name="colorAccent">@color/colorPrimary</item>

    </style>
    <style name="MyAwesomeTheme3" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/purple</item>
        <item name="colorPrimaryDark">@color/purpleDark</item>
        <item name="colorAccent">@color/teal3</item>
    </style>
</resources>
